public class bitwise {
    public static void main(String[] args) {
        int x = 4;
		int y =10
        System.out.println(x<y&&x<=y);
        System.out.println( x>y&&x>=y);
		System.out.println( x<y&x>y);
		System.out.println( x<y|y<x);
		System.out.println( x<y||y<x);
		System.out.println( x^y);
		System.out.println( ~x);
    }
}